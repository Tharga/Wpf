		<ImplicitUsings>enable</ImplicitUsings>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Close.svg" />
		<None Remove="Exit.svg" />
	</ItemGroup>

	<ItemGroup>
		<Resource Include="Exit.svg" />
