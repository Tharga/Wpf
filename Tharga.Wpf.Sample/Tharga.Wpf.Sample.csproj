		<PackageReference Include="System.Text.Json" Version="9.0.5" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Tharga.Wpf\Tharga.Wpf.csproj" />
	</ItemGroup>

</Project>